{
    "name": "cuongpham2107/filament-scaffold",
    "description": "Filament scaffold generator for Laravel - supports Filament 4.x",
    "keywords": [
        "filament",
        "laravel",
        "filament-scaffold",
        "scaffold-generator"
    ],
    "homepage": "https://github.com/cuongpham2107/filament-scaffold",
    "support": {
        "issues": "https://github.com/cuongpham2107/filament-scaffold/issues",
        "source": "https://github.com/cuongpham2107/filament-scaffold"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "cuongpham2107",
            "email": "cuongpham2107@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.1",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "nunomaduro/collision": "^8.5",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "spatie/laravel-ray": "^1.40"
    },
    "autoload": {
        "psr-4": {
            "Cuongpham\\FilamentScaffold\\": "src/",
            "Cuongpham\\FilamentScaffold\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cuongpham\\FilamentScaffold\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cuongpham\\FilamentScaffold\\FilamentScaffoldServiceProvider"
            ],
            "aliases": {
                "FilamentScaffold": "Cuongpham\\FilamentScaffold\\Facades\\FilamentScaffold"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
